@{
    Layout = "_LayoutAdmin";
}
@model List<Cliente>
<div class="row">
            <table class="table table-striped table-hover tabela">
                <h2 class="text-center">Cadastro de Usu√°rio</h2>
                <thead>
                    <tr>
                        <th scope="col">Nome</th>
                        <th scope="col">Telefone</th>
                        <th scope="col">CPF</th>
                        <th scope="col">Email</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var cliente in Model)
                    {
                        <tr>
                        <th scope="row">@cliente.Nome</th>
                        <td>@cliente.Telefone</td>
                        <td>@cliente.Cpf</td>
                        <td>@cliente.Email</td>
                    </tr>
                    }
                    
                </tbody>
            </table>
        </div>